      <static>true</static>
    </include>

    <!-- Shelf -->
    <include>
      <name>shelf</name>
      <pose> 8.40 -4.70 0.1 0 0 0 </pose>
      <uri>model://shelf</uri>
      <static>true</static>
    </include>

    <!-- Package to be picked up -->
    <include>
      <name>package</name>
      <pose>-7.5 2.5 0.5 0 0 0</pose>
      <uri>model://intruder</uri>
    </include>

