<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="leonardo_race_field">
    
    <physics name='default_physics' default='0' type='ode'>
      <gravity>0 0 -9.8066</gravity>
      <ode>
        <solver>
          <type>quick</type>
          <iters>10</iters>
          <sor>1.3</sor>
          <use_dynamic_moi_rescaling>0</use_dynamic_moi_rescaling>
          <precon_iterations>0</precon_iterations>
        </solver>
        <constraints>
          <cfm>0</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>100</contact_max_correcting_vel>
          <contact_surface_layer>0.001</contact_surface_layer>
          <max_contacts>5</max_contacts>
        </constraints>
      </ode>
      <max_step_size>0.025</max_step_size>
      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>40</real_time_update_rate>
    </physics>

    <light type="directional" name="sun">
      <cast_shadows>false</cast_shadows>
      <pose>0 0 10 0 0 0</pose>
      <diffuse>0.6 0.4 0.2 1</diffuse>
      <specular>0.2 0.2 0.2 1</specular>
      <attenuation>
        <range>1000</range>
        <constant>0.9</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
      <direction>-0.5 0.1 -0.9</direction>
    </light>

    <include>
      <name>race_field</name>
      <pose> 10 5 0.1 0 0 3.14159 </pose>
      <uri>model://race_field</uri>
      <static>true</static>
    </include>
    
    <!-- Roof -->
     <include>
      <name>roof</name>
      <pose> 10 -5 3.1 3.14159 0 3.14159 </pose>
      <uri>model://roof</uri>
      <static>true</static>
    </include>

    <!-- Tunnels -->
    <include>
      <name>rect_tunnel </name> 
      <pose> -4.8 3.6 0.1 0 0 0 </pose>
      <uri>model://rect_tunnel</uri>
      <static>true</static>
    </include>

    
    <include>
      <name>start_pod </name> 
      <pose> -9 3.5 0.1 0 0 -1.57 </pose>
      <uri>model://start_pod</uri>
      <static>true</static>
    </include> 

    <!--Obstacles-->
    <include>
      <name>obstacle_03 </name> 
      <pose> 3 -4.5 0.1 0 0 1.57 </pose>
      <uri>model://obstacle_06</uri>
      <static>true</static>
    </include>

    <include>
      <name>unk_obst</name> 
      <pose> 2.6 -1.90 0.1 0 0 0 </pose>
      <uri>model://unknown_obst_1</uri>
      <static>true</static>
    </include>

    <include>
      <name>unk_obst_2</name>
      <pose> -1.10 1.40 0.1 0 0 1.57 </pose>
