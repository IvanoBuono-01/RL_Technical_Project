  <model name="package1">
    <pose>-7.8 3.10 1.0 0 0 0</pose>  <!-- per test solo su robot posizione 9.20 -4 0.8 sennò nel cassone -7.8 3.10 1.0 0 0 0-->
    <static>false</static> 
    <link name="package_link">

      <inertial>
        <mass>0.1</mass> <inertia>
          <ixx>0.000027</ixx>
          <iyy>0.000027</iyy>
          <izz>0.0000375</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
        <linear_damping>10.0</linear_damping>
        <angular_damping>10.0</angular_damping>
      </inertial>
      
      <collision name="collision">
        <geometry><box><size>0.1 0.1 0.1</size></box></geometry>
          <surface>
              <friction>
                <ode>
                  <mu>1000000.0</mu> 
                  <mu2>50000.0</mu2>
                </ode>
              </friction>
            <contact>
              <ode>
                <kp>10.0</kp> 
                <kd>1.0</kd>
                <min_depth>0.001</min_depth> 
                <max_vel>0.1</max_vel>
              </ode>
            </contact>
          </surface>
      </collision>
      <visual name="visual">
        <geometry><box><size>0.1 0.1 0.1</size></box></geometry>
        <material>
          <ambient>1 0.5 0 1</ambient>
          <diffuse>1 0.5 0 1</diffuse>
        </material>
      </visual>
    </link>

    <include>
      <uri>model://aruco0</uri>
      <pose>0 0 0.051 0 0 4.71</pose> 
    </include>

    <joint name="tag_joint_1" type="fixed">
      <parent>package_link</parent>
      <child>aruco0::aruco_link</child> 
    </joint>
  
  </model>

